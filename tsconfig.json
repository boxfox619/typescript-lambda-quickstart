{
  "compilerOptions": {
    "lib": ["es2017"],
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "target": "es2017",
    "outDir": "dist",
    "baseUrl": "src",
    "paths": {
      "handler/*": ["handler/*"],
      "models/*": ["models/*"],
      "service/*": ["service/*"],
      "util/*": ["util/*"]
    }
  },
  "exclude": ["node_modules", "**/*.test.ts"]
}
